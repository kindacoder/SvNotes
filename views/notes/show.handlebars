<h5 style="padding:10px;" class="center teal">Notes</h5>
<div class="divider"></div>
<div class="row">
  <div class="col s12 m8">
    <div class="card hoverable">
      <div class="row">
        <div id="show-page" class="col s12 m7">
          <h5><i class="fa fa-graduation-cap"></i> Semester : {{data.semester}}</h5>
    <h5><i class="fa fa-angle-double-right"></i> Branch : {{data.branch}}</h5>
    <h5 class="highlight " ><i class="fa fa-book"></i> Subject : {{data.subject}}</h5>

        </div>
        <div class="col s12 m4">
            <a target="_blank" id="download-btn"class="button red btn main-button" href="/{{data.path}}"><i class="fa fa-download"></i>  Download This Note</a>
        </div>
      </div>
    </div>
    <div class="card hoverable">
      <div class="card-content">
        <span class="card-title">{{formatDate story.date 'MMMM Do YYYY'}}</span>
        {{{data.description}}}
      </div>
    </div>
    </div>
  <div class="col s12 m4">
    <div class="card center-align">
      <div class="card-content">
        <span class="card-title">{{data.user.firstName}} {{data.user.lastName}}</span>
        <img src="{{data.user.image}}" class="circle responsive-img">
      </div>
      <div class="card-action">
        <a class="" href="/notes/user/{{data.user.id}}" download>More From {{data.user.firstName}}</a>
      </div>
    </div>
  </div>
</div>